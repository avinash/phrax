Makefile: fractal.pro  /usr/lib/qt-3.3/mkspecs/default/qmake.conf /usr/lib/qt-3.3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile fractal.pro
qmake: 
	@$(QMAKE) -o Makefile fractal.pro

dist: 
	@mkdir -p .obj/fractal && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/fractal/ && $(COPY_FILE) --parents images/gtk/gtk-redo.png images/gtk/gnome-unknown.png images/gtk/gtk-print.png images/gtk/gtk-save-as.png images/gtk/gtk-save.png images/gtk/gtk-stock-fullscreen.png images/gtk/gtk-undo.png images/gtk/panel-logout.png images/gtk/stock-import.png images/gtk/stock-new.png images/gtk/stock-open.png images/gtk/stock-zoom-in.png images/gtk/stock-zoom-out.png images/gtk/handbook.png images/gtk/gtk-refresh.png .obj/fractal/ && $(COPY_FILE) --parents mainform.ui.h .obj/fractal/ && ( cd `dirname .obj/fractal` && $(TAR) fractal.tar fractal && $(GZIP) fractal.tar ) && $(MOVE) `dirname .obj/fractal`/fractal.tar.gz . && $(DEL_FILE) -r .obj/fractal

