.moc/moc_fractal.cpp: $(MOC) fractal.h
	$(MOC) fractal.h -o .moc/moc_fractal.cpp

.moc/moc_helpwindow.cpp: $(MOC) helpwindow.h
	$(MOC) helpwindow.h -o .moc/moc_helpwindow.cpp

.moc/moc_mainform.cpp: $(MOC) .ui/mainform.h
